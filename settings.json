  "editor.tokenColorCustomizations": {
    "[Seti Black]": 
      { 
        "textMateRules": [
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground":"#e7983e" //"#d19438"
            }
        },
        {
          "scope": [
            "punctuation.separator.pointer-access.cpp"
          ],
          "settings": {
            "foreground": "#d7d7d7ff"
          }
        },
        {
          "scope": [
            "keyword.control.return"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#fd2868"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "foreground": "#4b93d6"
          }
        },
        {
          "scope": [
            "keyword.control.directive", 
            "keyword.other.typedef", 
            "keyword.control.import", 
            "entity.name.function.decorator.python"
          ],
          "settings": {
            "foreground": "#fd2868"//"#e6008e"
          }
        },
        {
          "scope": [
            "keyword.operator", 
            "keyword.operator.logical", 
            "keyword.operator.assignment"
          ],
          "settings": {
            "foreground": "#fd2868"
          }
        },
        {
          "scope": "storage.modifier.reference",
          "settings": {
            "foreground": "#b1b1b1"
          }
        },
        {
          "scope": [
            "entity.name.function",
            "entity.name.class", 
            "entity.other.inherited-class", 
            "support.function", 
            "support.class"],
          "settings": {
            "fontStyle": "",
            "foreground": "#7dd443"
          }
        },
        {
          "scope": [
            "storage.type",
            "entity.name.type"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#6CA9E1"//"#0fa6cc"
          }
        },
        {
          "scope": [
            "storage.modifier.specifier.const.cpp", 
            "storage.type.struct",
            "storage.type.class"
            ],
          "settings": {
            "foreground": "#0de6b7"
          }
        },
        {
          "scope": [
            "string.quoted.double", 
            "string.quoted.single"
          ],
          "settings": {
            "foreground": "#ecea70"
          }
        },
        {
          "scope":[
            "storage.type.class.doxygen.cpp",
          ],
          "settings": {
            "foreground": "#999797",
            "fontStyle": "bold"
          }
        }
      ]
      },
      
      "[Base16 Tomorrow Dark]": 
      {
        "textMateRules": [
          {
            "scope": ["variable"],
            "settings": {
              "foreground": "#c5c8c6",
              "fontStyle": ""
            }
          },
          {
            "scope": ["variable.parameter"],
            "settings": {
              "foreground": "#c97373"
            }
          }
        ]
      }

  },
  "workbench.colorTheme": "Seti Black",

  "editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
  "editor.fontSize": 13.5,
  "editor.lineHeight": 22,
  "editor.fontLigatures": false,
  "debug.console.fontSize": 13.5,
   
  "python.dataScience.askForKernelRestart": false,
  "python.dataScience.runStartupCommands": ["%config IPCompleter.use_jedi = False"],
  "python.languageServer": "Jedi",
  "python.dataScience.interactiveWindowMode": "perFile",
  "window.titleSeparator": " ~ ",
  "todo-tree.tree.showScanModeButton": false,
}