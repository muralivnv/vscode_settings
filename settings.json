{
    "editor.tabSize": 2,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.enhancedColorization": "Enabled",
    "cmake.cmakePath": "C:/Program Files/CMake/bin/cmake",
    "cmake.preferredGenerators": [
        "Ninja",
